<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cat id="ShareETA#MissingContactInformation" xmlns="urn:apple:names:siri:cat:1.0" locale="en" >
  
  <conditions>
      <condition name="firstThreeTimes"><expression name="dialogState.usageCount" op="le" value="3"/></condition>
  </conditions>

  <all>
      <dialog>Sorry, I don't have enough contact info to share your ETA<opt> with <var name="recipient.conversationalName"/></opt>.</dialog>
      <dialog id="firstThreeResponse" condition="firstThreeTimes"> You can add it in Contacts, when it's safe to do so.</dialog>
  </all>
</cat>

